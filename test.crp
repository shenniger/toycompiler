; A sample file
(struct Point ((i32 x y)))
(static-run (
  (add-member-macro (find-struct "Point") "both" 
    (lambda (a name) (quasiquote (+ (memb (quasiunquote a) x) (memb (quasiunquote a) y)))))))
(defun main () i32 (
  (var Point a)
  (set (memb a x) 10)
  (set (memb a y) 12)
  (memb a both)))
